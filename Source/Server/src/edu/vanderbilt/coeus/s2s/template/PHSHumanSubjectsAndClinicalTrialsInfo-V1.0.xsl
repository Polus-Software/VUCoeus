le-cell height="14px">										<fo:block>											<xsl:text>&#xA;</xsl:text>										</fo:block>									</fo:table-cell>								</fo:table-row>																<!-- Section 5 - Other Clinical Trial-related Attachments -->								<fo:table-row>									<fo:table-cell number-columns-spanned="3" padding="4px" padding-top="6px" padding-bottom="6px"										border-top="1px solid black" border-bottom="1px solid black"										font-weight="bold">										<fo:block>											Section 5 - Other Clinical Trial-related Attachments										</fo:block>									</fo:table-cell>								</fo:table-row>								<xsl:for-each select="HumanSubjectStudy:OtherClinicalTrialAttachment">									<xsl:for-each select="att:AttachedFile">										<fo:table-row>											<xsl:if test="att:FileName != ''">												<xsl:call-template name="attachment">													<xsl:with-param name="filename">														<xsl:value-of select="att:FileName" />													</xsl:with-param>													<xsl:with-param name="mimetype">														<xsl:value-of select="att:MimeType" />													</xsl:with-param>													<xsl:with-param name="margin-left">20</xsl:with-param>													<xsl:with-param name="border-top">none</xsl:with-param>												</xsl:call-template>											</xsl:if>										</fo:table-row>										</xsl:for-each>									</xsl:for-each>							</fo:table-body>						</fo:table>											</fo:flow>				</fo:page-sequence>			</xsl:for-each>		</fo:root>	</xsl:template>		<!-- Arrays for IER data - START -->		<xsl:variable name="ethnicities">		<ethnicity>NotHispanic</ethnicity>		<ethnicity>Hispanic</ethnicity>		<ethnicity>UnknownEthnicity</ethnicity>	</xsl:variable>		<xsl:variable name="genders">		<gender>Female</gender>		<gender>Male</gender>		<gender>UnknownGender</gender>	</xsl:variable>	<xsl:variable name="races">		<race>			<nodename>AmericanIndian</nodename>			<desc>American Indian / Alaska Native</desc>		</race>		<race>			<nodename>Asian</nodename>			<desc>Asian</desc>		</race>		<race>			<nodename>Hawaiian</nodename>			<desc>Native Hawaiian or Other Pacific Islander</desc>		</race>		<race>			<nodename>Black</nodename>			<desc>Black or African American</desc>		</race>		<race>			<nodename>White</nodename>			<desc>White</desc>		</race>		<race>			<nodename>MultipleRace</nodename>			<desc>More than One Race</desc>		</race>		<race>			<nodename>UnknownRace</nodename>			<desc>Unknown or Not Reported</desc>		</race>		<race>			<nodename>Total</nodename>			<desc>Total</desc>		</race>	</xsl:variable>	<!-- Arrays for IER data - END -->	<!-- Planned Report template -->	<xsl:template match="HumanSubjectStudy:Planned">		<xsl:variable name="planned" select="." />				<fo:block font-weight="bold" line-height="20px" padding-top="20px">Planned</fo:block>		<fo:table width="100%" table-layout="fixed" font-size="8pt" border="1px solid black">			<fo:table-column column-width="30%"/>			<fo:table-column column-width="14%"/>			<fo:table-column column-width="14%"/>			<fo:table-column column-width="14%"/>			<fo:table-column column-width="14%"/>			<fo:table-column column-width="14%"/>			<fo:table-body>				<fo:table-row>					<fo:table-cell padding="4px" font-weight="bold" font-size="10pt" display-align="center"						number-rows-spanned="3" text-align="center" border-right="1px solid black" border-bottom="1px solid black">						<fo:block>							Racial Categories						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-weight="bold" font-size="10pt" 						number-columns-spanned="5" text-align="center" border-bottom="1px solid black">						<fo:block>							Ethnic Categories						</fo:block>					</fo:table-cell>				</fo:table-row>				<fo:table-row>					<fo:table-cell padding="4px" font-size="10pt" number-columns-spanned="2" text-align="center" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Not Hispanic or Latino						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" number-columns-spanned="2" text-align="center" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Hispanic or Latino						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-weight="bold" font-size="10pt" 						text-align="center" border-bottom="1px solid black">						<fo:block>							Total						</fo:block>					</fo:table-cell>				</fo:table-row>				<fo:table-row>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Female						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Male						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Female						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Male						</fo:block>					</fo:table-cell>					<fo:table-cell border-bottom="1px solid black">						<fo:block />					</fo:table-cell>				</fo:table-row>				<xsl:call-template name="reportSetup">					<xsl:with-param name="data"><xsl:value-of select="$planned" /></xsl:with-param>					<xsl:with-param name="reportType">P</xsl:with-param>				</xsl:call-template>						</fo:table-body>		</fo:table>	</xsl:template>		<!-- Cumulative inclusion template -->	<xsl:template match="HumanSubjectStudy:Cumulative">		<xsl:variable name="cumulative" select="." />				<fo:block font-weight="bold" line-height="20px" padding-top="20px">Cumulative (Actual)</fo:block>		<fo:table width="100%" table-layout="fixed" font-size="8pt" border="1px solid black">			<fo:table-column column-width="26%"/>			<fo:table-column column-width="7%"/>			<fo:table-column column-width="7%"/>			<fo:table-column column-width="8%"/>			<fo:table-column column-width="7%"/>			<fo:table-column column-width="7%"/>			<fo:table-column column-width="8%"/>			<fo:table-column column-width="7%"/>			<fo:table-column column-width="7%"/>			<fo:table-column column-width="8%"/>			<fo:table-column column-width="8%"/>			<fo:table-body>				<fo:table-row>					<fo:table-cell padding="4px" font-weight="bold" font-size="10pt" display-align="center"						number-rows-spanned="3" text-align="center" border-right="1px solid black" border-bottom="1px solid black">						<fo:block>							Racial Categories						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-weight="bold" font-size="10pt" 						number-columns-spanned="10" text-align="center" border-bottom="1px solid black">						<fo:block>							Ethnic Categories						</fo:block>					</fo:table-cell>				</fo:table-row>				<fo:table-row>					<fo:table-cell padding="4px" font-size="10pt" number-columns-spanned="3" text-align="center" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Not Hispanic or Latino						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" number-columns-spanned="3" text-align="center" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Hispanic or Latino						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" number-columns-spanned="3" text-align="center" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Unknown/Not Reported Ethnicity						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-weight="bold" font-size="10pt" 						text-align="center" border-bottom="1px solid black">						<fo:block>							Total						</fo:block>					</fo:table-cell>				</fo:table-row>				<fo:table-row>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Female						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Male						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Unknown/Not Reported						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Female						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Male						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Unknown/Not Reported						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Female						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black">						<fo:block>							Male						</fo:block>					</fo:table-cell>					<fo:table-cell padding="4px" font-size="10pt" text-align="center" font-weight="bold" 						border-bottom="1px solid black" border-right="1px solid black" >						<fo:block>							Unknown/Not Reported						</fo:block>					</fo:table-cell>					<fo:table-cell border-bottom="1px solid black">						<fo:block />					</fo:table-cell>				</fo:table-row>								<xsl:call-template name="reportSetup">					<xsl:with-param name="data"><xsl:value-of select="$cumulative" /></xsl:with-param>					<xsl:with-param name="reportType">C</xsl:with-param>				</xsl:call-template>							</fo:table-body>		</fo:table>	</xsl:template>		<xsl:template name="reportSetup">		<xsl:param name="data" />		<xsl:param name="reportType" />				<xsl:for-each select="exsl:node-set($races)/race">			<xsl:variable name="race" select="." />							<xsl:if test="($reportType = 'P' and not($race/nodename = 'UnknownRace')) or $reportType = 'C'">				<fo:table-row>					<xsl:choose>						<xsl:when test="$race/nodename = 'Total'">							<fo:table-cell padding="4px" font-size="10pt" border-right="1px solid black">								<fo:block>									<xsl:value-of select="$race/desc" />								</fo:block>							</fo:table-cell>							<xsl:call-template name="report">								<xsl:with-param name="raceID"><xsl:value-of select="$race/nodename" /></xsl:with-param>								<xsl:with-param name="data"><xsl:value-of select="$data" /></xsl:with-param>								<xsl:with-param name="reportType"><xsl:value-of select="$reportType" /></xsl:with-param>							</xsl:call-template>														<xsl:call-template name="totals">								<xsl:with-param name="raceID"><xsl:value-of select="$race/nodename" /></xsl:with-param>								<xsl:with-param name="data"><xsl:value-of select="$data" /></xsl:with-param>								<xsl:with-param name="reportType"><xsl:value-of select="$reportType" /></xsl:with-param>							</xsl:call-template>						</xsl:when>						<xsl:otherwise>							<fo:table-cell padding="4px" font-size="10pt" border-bottom="1px solid black" border-right="1px solid black">								<fo:block>									<xsl:value-of select="$race/desc" />								</fo:block>							</fo:table-cell>							<xsl:call-template name="report">								<xsl:with-param name="raceID"><xsl:value-of select="$race/nodename" /></xsl:with-param>								<xsl:with-param name="data"><xsl:value-of select="$data" /></xsl:with-param>								<xsl:with-param name="reportType"><xsl:value-of select="$reportType" /></xsl:with-param>							</xsl:call-template>														<xsl:call-template name="totals">								<xsl:with-param name="raceID"><xsl:value-of select="$race/nodename" /></xsl:with-param>								<xsl:with-param name="data"><xsl:value-of select="$data" /></xsl:with-param>								<xsl:with-param name="reportType"><xsl:value-of select="$reportType" /></xsl:with-param>							</xsl:call-template>						</xsl:otherwise>					</xsl:choose>				</fo:table-row>			</xsl:if>		</xsl:for-each>	</xsl:template>	 	<!-- Report data template -->	<xsl:template name="report">		<xsl:param name="raceID" />		<xsl:param name="data" />		<xsl:param name="reportType" />		<xsl:for-each select="$data/*">			<xsl:if test="($reportType = 'P' and not(local-name() = 'UnknownEthnicity')) or $reportType = 'C'">				<xsl:call-template name="ethnicities">					<xsl:with-param name="ethName" select="local-name()" />					<xsl:with-param name="ethData" select="." />					<xsl:with-param name="raceID" select="$raceID" />					<xsl:with-param name="reportType" select="$reportType" />				</xsl:call-template>			</xsl:if>		</xsl:for-each>	</xsl:template>	<!-- Ethnicities -->	<xsl:template name="ethnicities">		<xsl:param name="ethName" />		<xsl:param name="ethData" />		<xsl:param name="raceID" />		<xsl:param name="reportType" />		<xsl:for-each select="exsl:node-set($ethnicities)/ethnicity">			<xsl:variable name="eth" select="." />			<xsl:if test="$eth = $ethName">				<xsl:for-each select="$ethData/*">					<xsl:if test="($reportType = 'P' and not(local-name() = 'UnknownGender')) or $reportType = 'C'">						<xsl:call-template name="genders">							<xsl:with-param name="gendName"><xsl:value-of select="local-name()" /></xsl:with-param>							<xsl:with-param name="gendData"><xsl:value-of select="." /></xsl:with-param>							<xsl:with-param name="raceID" select="$raceID" />							<xsl:with-param name="reportType"><xsl:value-of select="$reportType" /></xsl:with-param>						</xsl:call-template>					</xsl:if>				</xsl:for-each>			</xsl:if>		</xsl:for-each>	</xsl:template>		<!-- Get data for a given race from a ethnicity / gender node -->	<xsl:template name="genders">		<xsl:param name="gendName" />		<xsl:param name="gendData" />		<xsl:param name="raceID" />		<xsl:param name="reportType" />				<xsl:for-each select="exsl:node-set($genders)/gender">			<xsl:variable name="gend" select="." /> 			<xsl:if test="$gend = $gendName">				<xsl:for-each select="$gendData/*">					<xsl:if test="$raceID = local-name()">						<xsl:if test="($reportType = 'P' and not(local-name() = 'UnknownRace')) or $reportType = 'C'">							<xsl:choose>								<xsl:when test="local-name() = 'Total'">									<fo:table-cell padding="4px" font-size="8pt" border-right="1px solid black" text-align="right">										<fo:block>											<xsl:value-of select="." />										</fo:block>									</fo:table-cell>								</xsl:when>								<xsl:otherwise>									<fo:table-cell padding="4px" font-size="8pt" text-align="right" 										border-bottom="1px solid black" border-right="1px solid black">										<fo:block>											<xsl:value-of select="." />										</fo:block>									</fo:table-cell>								</xsl:otherwise>							</xsl:choose>						</xsl:if>					</xsl:if>				</xsl:for-each>			</xsl:if>		</xsl:for-each>	</xsl:template>		<!-- Get race totals -->	<xsl:template name="totals">		<xsl:param name="raceID" />		<xsl:param name="data" />		<xsl:param name="reportType" />				<xsl:for-each select="$data/*">			<xsl:if test="local-name() = 'Total'">				<xsl:variable name="raceTotals" select="." />				<xsl:for-each select="$raceTotals/*">					<xsl:if test="local-name() = $raceID">						<xsl:choose>							<xsl:when test="local-name() = 'Total'">								<fo:table-cell padding="4px" font-size="8pt" text-align="right">									<fo:block>										<xsl:value-of select="." />									</fo:block>								</fo:table-cell>							</xsl:when>							<xsl:otherwise>								<fo:table-cell padding="4px" font-size="8pt" border-bottom="1px solid black" text-align="right">									<fo:block>										<xsl:value-of select="." />									</fo:block>								</fo:table-cell>															</xsl:otherwise>						</xsl:choose>					</xsl:if>				</xsl:for-each>			</xsl:if>		</xsl:for-each>	</xsl:template>		<!-- ================================ -->	<!-- Format Date template -->	<!-- ================================ -->	<xsl:template name="formatDate">		<xsl:param name="value"/>		<xsl:if test="string-length($value) &gt; 0 and $value != '--'">			<xsl:value-of select="format-number(substring($value,6,2), '00')"/>			<xsl:text>-</xsl:text>			<xsl:value-of select="format-number(substring($value,9,2), '00')"/>			<xsl:text>-</xsl:text>			<xsl:value-of select="format-number(substring($value,1,4), '0000')"/>		</xsl:if>	</xsl:template>		<!-- Radio  template -->	<xsl:template name="radio">		<xsl:param name="value"/>		<xsl:param name="radio_on_value"/>		<xsl:choose>			<xsl:when test="$value = $radio_on_value">				<fo:inline white-space-collapse="false" font-family="ZapfDingbats" font-size="10pt" padding-start="1pt" padding-end="1pt">&#x25cf;</fo:inline>			</xsl:when>			<xsl:otherwise>				<fo:inline white-space-collapse="false" font-family="ZapfDingbats" font-size="10pt" padding-start="1pt" padding-end="1pt">&#x274d;</fo:inline>			</xsl:otherwise>		</xsl:choose>	</xsl:template>	<!-- Checkbox templates -->		<xsl:template name="checkboxY">		<xsl:param name="value" />		<xsl:param name="schemaChoice">Y: Yes</xsl:param>		<xsl:if test="normalize-space($value) = normalize-space($schemaChoice)">			<fo:inline xmlns:fo="http://www.w3.org/1999/XSL/Format"				xmlns:footer="http://apply.grants.gov/system/Footer-V1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema"				xmlns:globLib="http://apply.grants.gov/system/GlobalLibrary-V2.0"				xmlns:glob="http://apply.grants.gov/system/Global-V1.0" font-family="ZapfDingbats"				font-size="8pt">&#x2714;</fo:inline>		</xsl:if>	</xsl:template>		<xsl:template name="checkboxN">		<xsl:param name="value" />		<xsl:param name="schemaChoice">N: No</xsl:param>		<xsl:if test="normalize-space($value) = normalize-space($schemaChoice)">			<fo:inline xmlns:fo="http://www.w3.org/1999/XSL/Format"				xmlns:footer="http://apply.grants.gov/system/Footer-V1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema"				xmlns:globLib="http://apply.grants.gov/system/GlobalLibrary-V2.0"				xmlns:glob="http://apply.grants.gov/system/Global-V1.0" font-family="ZapfDingbats"				font-size="8pt">&#x2714;</fo:inline>		</xsl:if>	</xsl:template>		<!--  Attachment template -->	<xsl:template name="attachment">		<xsl:param name="filename" />		<xsl:param name="mimetype" />		<xsl:param name="margin-left" />		<xsl:param name="border-top" />				<xsl:element name="fo:table-cell">			<xsl:if test="$border-top  != ''">				<xsl:attribute name="border-top">					<xsl:value-of select="$border-top" />				</xsl:attribute>			</xsl:if>			<xsl:element name="fo:block">				<xsl:attribute name="margin">4px</xsl:attribute>				<xsl:attribute name="margin-left">					<xsl:value-of select="$margin-left" />px				</xsl:attribute>				<xsl:value-of select="$filename" />			</xsl:element>		</xsl:element>		<fo:table-cell>			<xsl:if test="$border-top  != ''">				<xsl:attribute name="border-top">					<xsl:value-of select="$border-top" />				</xsl:attribute>			</xsl:if>			<fo:block margin="4px">				<xsl:if test="$filename  != ''">					Mime Type: <xsl:value-of select="$mimetype"/>				</xsl:if>			</fo:block>		</fo:table-cell>	</xsl:template></xsl:stylesheet>