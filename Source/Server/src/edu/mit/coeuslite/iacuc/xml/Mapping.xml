<?xml version = "1.0" encoding = "UTF-8"?>
<PROCEDURE_DETAILS>
	<GET_PERSON_DETAILS>
		<procedure>GET_POC_DATA</procedure>
		<map>
			<procparameter>PERSON_ID</procparameter>
			<property>personId</property>
			<type>Integer</type>
			<proc_type>IN</proc_type>
		</map>

		<map>
			<procparameter>NAME</procparameter>
			<property>personName</property>
			<type>String</type>

		</map>
		<map>
			<procparameter>AGE</procparameter>
			<property>age</property>
			<type>Integer</type>

		</map>
		<map>
			<procparameter>DOB</procparameter>
			<property>dob</property>
			<type>Date</type>

		</map>
		<map>
			<procparameter>SALARY</procparameter>
			<property>salary</property>
			<type>Double</type>

		</map>
		<map>
			<procparameter>MARTIAL_STATUS</procparameter>
			<property>maritalStatus</property>
			<type>Boolean</type>

		</map>

		<map>
			<procparameter>EMAIL_ID</procparameter>
			<property>emailId</property>
			<type>String</type>

		</map>
		<map>
			<procparameter>UPDATE_TIMESTAMP</procparameter>
			<property>timeStamp</property>
			<type>Timestamp</type>

		</map>

		<map>
			<procparameter>AC_TYPE</procparameter>
			<property>acType</property>
			<type>String</type>

		</map>
		<map>
			<procparameter>RESULTSET</procparameter>
			<property>resultset</property>
			<type>resultset</type>
			<proc_type>OUT</proc_type>
		</map>

	</GET_PERSON_DETAILS>


