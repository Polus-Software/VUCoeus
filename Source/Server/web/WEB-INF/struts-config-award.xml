<struts-config>
    <form-beans>
        <!--Start Form Beans @Author Sony-->
        <form-bean name="propBasedDiclForm" type="edu.mit.coeuslite.coiv2.formbeans.propBasedDiclForm"></form-bean>
        <form-bean name="coiDisclosureForm" type="edu.mit.coeuslite.coiv2.beans.CoiDisclosureBean"></form-bean>
        <form-bean name="assignDisclToUser" type="edu.mit.coeuslite.coiv2.formbeans.Coiv2AssignDisclToUser"></form-bean>
         <form-bean name="dispDisclStatusForm" type="edu.mit.coeuslite.coiv2.formbeans.Coiv2DispDisclStatusForm"></form-bean>
